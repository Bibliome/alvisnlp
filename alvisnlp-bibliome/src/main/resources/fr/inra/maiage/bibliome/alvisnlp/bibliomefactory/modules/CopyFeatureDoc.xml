<?xml version="1.0" encoding="UTF-8"?>
<alvisnlp-doc author="" beta="true" date=""
	short-target="CopyFeature"
	target="fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.CopyFeature">
	<synopsis>
		<p>Copy the value of a feature in another feature for a selection of elements.</p>
	</synopsis>

	<module-doc>
		<description>
			<p><this/> evaluates <param>target</param> as a list of elements. For each element <this/> copies the last value of <param>sourceFeatureName</param> into <param>targetFeatureName</param>.</p>
			<p><this/> is useful to build plans that select a particular feature to process.</p>
		</description>

		<param-doc mandatory="required" name="sourceFeatureName"
			name-type="feature" short-type="String" type="java.lang.String">
			<p>UNDOCUMENTED</p>
		</param-doc>

		<param-doc mandatory="required" name="target"
			short-type="Expression"
			type="fr.inra.maiage.bibliome.alvisnlp.core.corpus.expressions.Expression">
			<p>UNDOCUMENTED</p>
		</param-doc>

		<param-doc mandatory="required" name="targetFeatureName"
			name-type="feature" short-type="String" type="java.lang.String">
			<p>UNDOCUMENTED</p>
		</param-doc>
		
        <include-doc>fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.CorpusModuleParamsDoc</include-doc>
	</module-doc>
</alvisnlp-doc>