<alvisnlp-doc author="Robert Bossy" date="2010-10-28">
  <synopsis>
    <p>Converts into a string of characters.</p>
  </synopsis>

  <converter-doc>
    <string-conversion>
      <p>The input string is stripped from leading and trailing whitespace characters.</p>
    </string-conversion>
  
    <xml-conversion>
      <p>
	<xverb><param value="STRING"/></xverb>
	or
	<xverb><param>STRING</param></xverb>
      </p>
    </xml-conversion>
  </converter-doc>
</alvisnlp-doc>
