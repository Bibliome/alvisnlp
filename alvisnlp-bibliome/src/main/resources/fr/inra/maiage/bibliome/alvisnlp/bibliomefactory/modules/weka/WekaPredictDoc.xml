<alvisnlp-doc author="Robert Bossy" date="2012-04-30" target="fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.newclassifiers.TaggingElementClassifier">
  <synopsis>
    <p>Classifies elements with a Weka classifier.</p>
  </synopsis>

  <module-doc>
    <description>
      <p><this/> applies the classifier loaded from <param name="classifier"/> on elements specified by <param name="examples"/>.</p>
    </description>
  
    <param-doc name="predictedClassFeatureKey">
      <p>Feature where to write the class prediction.</p>
    </param-doc>
  
    <param-doc name="classifierFile">
      <p>Serialized classifier file. This file must be generated by <module name="TrainingElementClassifier"/> with the same <param name="relationDefinition"/>.</p>
    </param-doc>

	<include-doc>fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.weka.ElementClassifierParamsDoc</include-doc>
</module-doc>
</alvisnlp-doc>
