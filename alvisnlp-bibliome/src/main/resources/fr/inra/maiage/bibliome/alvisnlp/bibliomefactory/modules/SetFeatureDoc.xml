<?xml version="1.0" encoding="UTF-8"?>
<alvisnlp-doc author="" beta="true" date="" short-target="SetFeature" target="fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.SetFeature">
	<synopsis>
		<p>Set the value of a feature for a selection of elements.</p>
	</synopsis>

	<module-doc>
		<description>
			<p><this/> evaluates <param>target</param> as a list of elements. For each element <this/> set value of <param>feature</param> to <param>value</param>.</p>
			<p><this/> is useful to build plans that select a particular feature to process.</p>
		</description>

		<param-doc name="target">
			<p>UNDOCUMENTED</p>
		</param-doc>

		<param-doc name="feature">
			<p>UNDOCUMENTED</p>
		</param-doc>

		<param-doc name="value">
			<p>UNDOCUMENTED</p>
		</param-doc>

		<param-doc name="featureName">
			<p>Deprecated alias for <param>feature</param>.</p>
		</param-doc>

		<param-doc name="featureValue">
			<p>Deprecated alias for <param>value</param>.</p>
		</param-doc>
		
        <include-doc>fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.CorpusModuleParamsDoc</include-doc>
	</module-doc>
</alvisnlp-doc>
