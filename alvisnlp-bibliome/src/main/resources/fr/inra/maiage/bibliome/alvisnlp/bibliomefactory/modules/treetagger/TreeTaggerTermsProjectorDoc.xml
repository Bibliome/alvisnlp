<?xml version="1.0" encoding="UTF-8"?>
<alvisnlp-doc author="" beta="false" date="" short-target="TreeTaggerTermsProjector" target="TreeTaggerTermsProjector">
  <synopsis>
    <p>Project terms from a lexicon in tree tagger format.</p>
  </synopsis>
  
  <module-doc>
    <description>
      <p><this/> reads <param>termsFile</param> and assumes a 3-column tree-tagger format. Entries shall be separated by a period (<em>./SENT/.</em>).</p>
    </description>

    <param-doc default-value="false" mandatory="default: false" name="lemmaKeys" short-type="Boolean" type="java.lang.Boolean">
      <p>Use lemmas as keys instead of the surface form.</p>
    </param-doc>

    <param-doc default-value="pos" mandatory="default: pos" name="posFeature" name-type="feature" short-type="String" type="java.lang.String">
      <p>Feature where to store the POS tag of matches.</p>
    </param-doc>

    <param-doc default-value="term" mandatory="default: term" name="termFeature" name-type="feature" short-type="String" type="java.lang.String">
      <p>Feature where to store the term surface form.</p>
    </param-doc>

    <param-doc default-value="lemma" mandatory="default: lemma" name="lemmaFeature" name-type="feature" short-type="String" type="java.lang.String">
      <p>Feature where to store the term lemma.</p>
    </param-doc>

    <param-doc default-value="pos" mandatory="default: pos" name="posFeatureName" name-type="feature" short-type="String" type="java.lang.String">
      <p>Deprecated alias for <param>posFeature</param>.</p>
    </param-doc>

    <param-doc default-value="term" mandatory="default: term" name="termFeatureName" name-type="feature" short-type="String" type="java.lang.String">
      <p>Deprecated alias for <param>termFeature</param>.</p>
    </param-doc>

    <param-doc default-value="lemma" mandatory="default: lemma" name="lemmaFeatureName" name-type="feature" short-type="String" type="java.lang.String">
      <p>Deprecated alias for <param>lemmaFeature</param>.</p>
    </param-doc>

    <param-doc mandatory="required" name="termsFile" short-type="SourceStream" type="fr.inra.maiage.bibliome.util.streams.SourceStream">
      <p>File where to read entries.</p>
    </param-doc>

	<include-doc>fr.inra.maiage.bibliome.alvisnlp.bibliomefactory.modules.trie.TrieProjectorParamsDoc</include-doc>
  </module-doc>
</alvisnlp-doc>
