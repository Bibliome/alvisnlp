    <oboFiles>OntoBiotope_BioNLP-ST-2016.obo</oboFiles>
  </read>
  
  <seg href="res://segmentation.plan"/>
  
  <genia class="GeniaTagger" />
  
  <train class="TomapTrain">
    <workingDir>yatea</workingDir>
    <outFile>tomap.xml</outFile>
    <conceptIdentifier>document.@id</conceptIdentifier>
  </train>
</alvisnlp-plan>
