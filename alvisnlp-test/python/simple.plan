<alvisnlp-plan id="python-simple">
  <read file="BioNLP-ST-2016_BB-event/read.plan"/>
  
  <features-before file="write-features.plan">
    <file>features-before.txt</file>
  </features-before>

  <simple class="PythonScript">
    <commandLine separator=" ">/home/rbossy/dist/anaconda3/bin/python /home/rbossy/code/alvisnlp/alvisnlp-test/python/simple.py</commandLine>
    <environment>PYTHONPATH=/home/rbossy/code/alvisnlp/share</environment>
  </simple>
  
  <features-after file="write-features.plan">
    <file>features-after.txt</file>
  </features-after>

  <update class="PythonScript">
    <commandLine separator=" ">/home/rbossy/dist/anaconda3/bin/python /home/rbossy/code/alvisnlp/alvisnlp-test/python/update.py</commandLine>
    <environment>PYTHONPATH=/home/rbossy/code/alvisnlp/share</environment>
    <documentFilter>@id == "BB-event-1016123" or @id == "BB-event-10658649"</documentFilter>
    <layerNames>Bacteria,Habitat</layerNames>
  </update>
  
  <features-update file="write-features.plan">
    <file>features-update.txt</file>
  </features-update>
</alvisnlp-plan>
